# Enable SPIRAM support
CONFIG_SPIRAM=y
CONFIG_ESP32S3_SPIRAM_MODE_OCT=y
CONFIG_ESP32S3_SPIRAM_TYPE_AUTO=y
CONFIG_ESP32S3_SPIRAM_SIZE=-1
CONFIG_ESP32S3_SPIRAM_SPEED_80M=y
CONFIG_ESP32S3_SPIRAM_BOOT_INIT=y
CONFIG_ESP32S3_SPIRAM_USE_MALLOC=y
CONFIG_ESP32S3_SPIRAM_MEMTEST=y
CONFIG_ESP32S3_SPIRAM_MALLOC_ALWAYSINTERNAL=16384
CONFIG_ESP32S3_SPIRAM_TRY_ALLOCATE_WIFI_LWIP=y
CONFIG_ESP32S3_SPIRAM_MALLOC_RESERVE_INTERNAL=32768
